        "Math",
        "Arbitrary-precision",
        "Arithmetic",
        "BigInteger",
        "BigDecimal",
        "BigRational"
    ],
    "license": "MIT",
    "require": {
        "php": ">=5.3"
    },
    "require-dev": {
        "phpunit/phpunit": "*",
        "satooshi/php-coveralls": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "Brick\\Math\\": "src/",
            "Brick\\Math\\Tests\\": "tests/"
        }
    }
}
